      'SELECT'
      '   ID_FORNECEDOR,'
      '   NOME,'
      '   BAIRRO,'
      '   ENDERECO,'
      '   NUMERO,'
      '   CEP,'
      '   ID_CIDADE,'
      '   ID_ESTADO,'
      '   TELEFONE,'
      '   CNPJ,'
      '   EMAIL,'
      '   CADASTRO'
      'FROM FORNECEDORES'
      'ORDER BY ID_FORNECEDOR;   ')
