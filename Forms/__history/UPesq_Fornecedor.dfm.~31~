  inherited qryPesq_Padrao: TFDQuery
    UpdateOptions.FetchGeneratorsPoint = gpDeferred
    UpdateOptions.GeneratorName = 'FORNECEDORES_FORNECEDORESID_FORNECEDOR_SEQ'
    UpdateOptions.AutoIncFields = 'ID_FORNECEDOR'
    SQL.Strings = (
      ' SELECT'
      '   F.ID_FORNECEDOR,'
      '   F.NOME,'
      '   F.ENDERECO,'
      '   F.NUMERO,'
      '   F.BAIRRO,'
      '   F.CEP,'
      '   D.CIDADE,'
      '   F.ID_CIDADE,'
      '   E.ESTADO,'
      '   F.ID_ESTADO,   '
      '   F.TELEFONE,'
      '   F.CNPJ,'
      '   F.EMAIL,'
      '   F.CADASTRO '
      'FROM FORNECEDORES F'
      'INNER JOIN CIDADES D'
      'ON F.ID_CIDADE = D.ID_CIDADE'
      'INNER JOIN ESTADOS E'
      'ON F.ID_ESTADO = E.ID_ESTADO'
      'ORDER BY ID_FORNECEDOR ')
    object qryPesq_Padraoid_fornecedor: TIntegerField
      AutoGenerateValue = arAutoInc
      DisplayLabel = 'ID'
      FieldName = 'id_fornecedor'
      Origin = 'id_fornecedor'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
    end
    object qryPesq_Padraonome: TWideStringField
      DisplayLabel = 'NOME'
      FieldName = 'nome'
